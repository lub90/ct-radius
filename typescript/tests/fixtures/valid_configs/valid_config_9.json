{
  "allowRequestedVlan": true,
  "vlanSeparator": "#",

  "requestRoutes": {
    "wifi": {
      "modules": [
        "root",
        "wifi-mid",
        "wifi-extended",
        "wifi-final"
      ]
    },
    "vpn": {
      "modules": [
        "root",
        "vpn-mid",
        "vpn-extended"
      ]
    }
  },

  "root": {
    "type": "ct-groups",

    "wifiAccessGroups": [1],
    "includeAssignmentGroupsInAccessGroups": true,

    "pathToPrivateDecryptionKey": "/keys/root.pem",
    "pathToCacheFile": "/cache/root.json",

    "backendConfig": {
      "timeout": 5,
      "usernameFieldName": "cmsUserId"
    },

    "vlanMapping": {
      "defaultVlan": 1,
      "assignments": {
        "1": 1
      },
      "assignmentsIfRequested": {
        "900": 900
      }
    }
  },

  "wifi-mid": {
    "inherits": "root",

    "wifiAccessGroups": [1, 10, 11],

    "vlanMapping": {
      "assignments": {
        "10": 10,
        "11": 11
      }
    }
  },

  "wifi-extended": {
    "inherits": "wifi-mid",

    "wifiAccessGroups": [1, 10, 11, 20],
    "includeAssignmentGroupsInAccessGroups": false,

    "pathToPrivateDecryptionKey": "/keys/wifi-extended.pem",

    "vlanMapping": {
      "assignmentsIfRequested": {
        "777": 777
      }
    }
  },

  "wifi-final": {
    "inherits": "wifi-extended",

    "wifiAccessGroups": [1, 10, 11, 20, 21, 22],

    "pathToPrivateDecryptionKey": "/keys/wifi-final.pem",

    "vlanMapping": {
      "defaultVlan": 30,
      "assignments": {
        "22": 30
      }
    }
  },

  "vpn-mid": {
    "inherits": "root",

    "wifiAccessGroups": [1, 50, 51],

    "pathToPrivateDecryptionKey": "/keys/vpn-mid.pem",

    "vlanMapping": {
      "assignments": {
        "50": 50,
        "51": 51
      }
    }
  },

  "vpn-extended": {
    "inherits": "vpn-mid",

    "wifiAccessGroups": [1, 50, 51, 60],
    "includeAssignmentGroupsInAccessGroups": false,

    "pathToPrivateDecryptionKey": "/keys/vpn-extended.pem",

    "vlanMapping": {
      "defaultVlan": 60,
      "assignmentsIfRequested": {
        "888": 888
      }
    }
  }
}
