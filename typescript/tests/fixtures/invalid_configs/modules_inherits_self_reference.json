{
  "allowRequestedVlan": true,
  "vlanSeparator": "#",

  "requestRoutes": {
    "wifi": { "modules": ["self"] },
    "vpn": { "modules": [] }
  },

  "self": {
    "type": "ct-groups",
    "inherits": "self",
    "wifiAccessGroups": [1],
    "includeAssignmentGroupsInAccessGroups": true,
    "pathToPrivateDecryptionKey": "/keys/key.pem",
    "pathToCacheFile": "/cache/cache.json",
    "backendConfig": { "timeout": 5, "usernameFieldName": "cmsUserId" },
    "vlanMapping": { "defaultVlan": 1, "assignments": {}, "assignmentsIfRequested": {} }
  }
}
