{
  "allowRequestedVlan": 1,
  "vlanSeparator": "#",

  "requestRoutes": {
    "wifi": {
      "modules": ["ct-groups"]
    },
    "vpn": {
      "modules": ["ct-groups"]
    }
  },

  "ct-groups": {
    "type": "ct-groups",

    "wifiAccessGroups": [101],
    "includeAssignmentGroupsInAccessGroups": true,

    "pathToPrivateDecryptionKey": "/ct-radius/key.pem",
    "pathToCacheFile": "/ct-radius/cache/personCache.json",

    "backendConfig": {
      "timeout": 5,
      "usernameFieldName": "cmsUserId"
    },

    "vlanMapping": {
      "defaultVlan": 10,
      "assignments": {},
      "assignmentsIfRequested": {}
    }
  }
}
