{
  "allowRequestedVlan": true,
  "vlanSeparator": "#",

  "requestRoutes": {
    "wifi": { "modules": ["ct-groups"] }
  },

  "ct-groups": {
    "type": "ct-groups",

    "wifiAccessGroups": [101],
    "includeAssignmentGroupsInAccessGroups": true,

    "pathToPrivateDecryptionKey": "/keys/key.pem",
    "pathToCacheFile": "/cache/cache.json",

    "backendConfig": {
      "timeout": 5,
      "usernameFieldName": "cmsUserId"
    },

    "vlanMapping": {
      "defaultVlan": 10,
      "assignments": {},
      "assignmentsIfRequested": {}
    }
  }
}
